{
  "name": "front-end-react",
  "version": "1.0.0",
  "description": "Front-end em React com aplicação de arquitetura limpa e testes unitários",
  "main": "index.js",
  "scripts": {
    "dev": "webpack-dev-server --hot",
    "start": "webpack-dev-server --open",
    "test": "jest --passWithNoTests --no-cache --runInBand",
    "test:watch": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage",
    "test:cache": "jest --clearCache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luizotavioautomacao/front-end-react.git"
  },
  "keywords": [
    "front-end",
    "react",
    "clean-code",
    "TDD"
  ],
  "author": "Luiz Otávio",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/luizotavioautomacao/front-end-react/issues"
  },
  "homepage": "https://github.com/luizotavioautomacao/front-end-react#readme",
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@testing-library/react": "^15.0.6",
    "@types/axios": "^0.14.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^11.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-standard": "^5.0.0",
    "git-commit-msg-linter": "^5.0.7",
    "husky": "^9.0.11",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-localstorage-mock": "^2.4.26",
    "lint-staged": "^15.2.2",
    "node-sass": "^9.0.0",
    "sass-loader": "^14.1.1",
